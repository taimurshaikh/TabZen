{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "TabZen",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "storage",
        "activeTab"
      ],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    }, 
    
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
      
}